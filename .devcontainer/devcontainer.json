{
	"name": "Ubuntu",
	"build": {
		"dockerfile": "Dockerfile",
		// Update 'VARIANT' to pick an Ubuntu version: focal, bionic
		"args": { "VARIANT": "focal" }
	},

	// Add the IDs of extensions you want installed when the container is created.
	"extensions": [
		"robbowen.synthwave-vscode",
		"ms-vscode.powershell"
	],

	"remoteUser": "vscode"
}